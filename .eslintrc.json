{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "preact",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["warn", "tab"],
		"no-unused-vars": "warn",		
		"no-multiple-empty-lines": "off",
		"no-tabs": "off",
		"quotes": ["warn", "single"],
		"jsx-quotes": ["warn", "prefer-single"],
		"eol-last": "off"
    },
    "parser": "@babel/eslint-parser"
}
